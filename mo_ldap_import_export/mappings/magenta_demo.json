{
  "ldap_to_mo": {
    "Employee": {
      "objectClass": "ramodels.mo.employee.Employee",
      "givenname": "{{ldap.givenName or ldap.name|splitlast|first}}",
      "surname": "{{ ldap.sn or ldap.name|splitlast|last or ''}}",
      "cpr_no": "{{ldap.employeeID|strip_non_digits or NONE}}",
      "seniority": "{{NONE}}",
      "nickname_givenname": "{{NONE}}",
      "nickname_surname": "{{NONE}}",
      "uuid": "{{ employee_uuid or NONE }}"
    },
    // Note: This key needs to be a valid MO address type user_key*
    "EmailEmployee": {
      "objectClass": "ramodels.mo.details.address.Address",
      "value": "{{ldap.mail or NONE}}",
      "type": "address",
      "validity": "{{ dict(from_date = now()|mo_datestring) }}",
      "address_type": "{{ dict(uuid=get_address_type_uuid('EmailEmployee')) }}",
      "person": "{{ dict(uuid=employee_uuid or NONE) }}"
    },
    // Note: This key needs to be a valid MO address type user_key*
    "LocationEmployee": {
      "objectClass": "ramodels.mo.details.address.Address",
      "value": "{{ ldap.postalAddress or NONE }}",
      "type": "address",
      "validity": "{{ dict(from_date=now()|mo_datestring) }}",
      "address_type": "{{ dict(uuid=get_address_type_uuid('LocationEmployee')) }}",
      "person": "{{ dict(uuid=employee_uuid or NONE) }}"
    },
    // Note: This key needs to be a valid MO IT-system user_key*
    "Active Directory": {
      "objectClass": "ramodels.mo.details.it_system.ITUser",
      "user_key": "{{ ldap.msSFU30Name or NONE }}",
      "itsystem": "{{ dict(uuid=get_it_system_uuid('Active Directory')) }}",
      "validity": "{{ dict(from_date=now()|mo_datestring) }}",
      "person": "{{ dict(uuid=employee_uuid or NONE) }}"
    },
    "Engagement": {
      "objectClass": "ramodels.mo.details.engagement.Engagement",
      "org_unit": "{{ dict(uuid=get_or_create_org_unit_uuid(ldap.division)) }}",
      "job_function": "{{ dict(uuid=get_job_function_uuid(ldap.title)) }}",
      "engagement_type": "{{ dict(uuid=get_engagement_type_uuid(ldap.employeeType)) }}",
      "user_key": "{{ ldap.personalTitle or uuid4() }}",
      "validity": "{{ dict(from_date=now()|mo_datestring) }}",
      "person": "{{ dict(uuid=employee_uuid or NONE) }}"
    }
  },
  "mo_to_ldap": {
    "Employee": {
      "objectClass": "user",
      "givenName": "{{mo_employee.givenname}}",
      "sn": "{{mo_employee.surname}}",
      "displayName": "{{mo_employee.surname}}, {{mo_employee.givenname}}",
      "name": "{{mo_employee.givenname}} {{mo_employee.surname}}",
      "employeeID": "{{mo_employee.cpr_no or NONE}}",
      "department": "{{NONE}}"
    },
    // Note: This key needs to be a valid MO address type name*
    "EmailEmployee": {
      "objectClass": "user",
      "mail": "{{mo_employee_address.value}}",
      "employeeID": "{{mo_employee.cpr_no}}"
    },
    // Note: This key needs to be a valid MO address type name*
    "LocationEmployee": {
      "objectClass": "user",
      "postalAddress": "{{mo_employee_address.value}}",
      "employeeID": "{{mo_employee.cpr_no}}"
    },
    // Note: This key needs to be a valid MO IT-system name*
    "Active Directory": {
      "objectClass": "user",
      "msSFU30Name" : "{{mo_employee_it_user.user_key}}",
      "employeeID": "{{mo_employee.cpr_no}}"
    },
    "Engagement" : {
      "objectClass": "user",
      "employeeID": "{{mo_employee.cpr_no}}",
      "department": "{{NONE}}",
      "company": "{{NONE}}",
      "departmentNumber": "{{NONE}}",
      "division": "{{get_org_unit_path_string(mo_employee_engagement.org_unit.uuid)}}",
      "primaryGroupID": "{{NONE}}",
      "employeeType": "{{get_engagement_type_name(mo_employee_engagement.engagement_type.uuid)}}",
      "memberOf": "{{NONE}}",
      "personalTitle": "{{mo_employee_engagement.user_key}}",
      "title": "{{get_job_function_name(mo_employee_engagement.job_function.uuid)}}"
    }
  },
